<Alloy>
	<Window id="game_container">
			<View id="topWrapper">
				<View id="leftCorner">
					<View id="backView" onClick="goBack">
						<Label class="backLabel" id="backLabel" />
					</View>					
				</View>
				<View id="rightCorner">
					<View id="timerView">
						<Label id="timerMainLabel">
							Time:
							<Label id="timerHour"></Label>
							<Label id="timerMinute"></Label>
							<Label id="timerSecond"></Label>
						</Label>
					</View>
				</View>
			</View>
			<ImageView id="topImage" image="logo.png"></ImageView>
				<TableView id="tableView" />
			<!--<View id="bottomWrapper">

		</View> -->
	</Window>
</Alloy>
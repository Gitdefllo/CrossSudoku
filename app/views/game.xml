<Alloy>
	<Window id="game_container">
		<View id="wrapper">
			<View id="topWrapper">
				<View id="leftCorner">
					<View id="backView" onClick="goBack">
						<Label id="backLabel">&lt; back</Label>
					</View>
				</View>
				<View id="rightCorner">
					<View id="timerView">
						<Label id="timerMainLabel">
							Time:
							<Label id="timerHour"></Label>
							<Label id="timerMinute"></Label>
							<Label id="timerSecond"></Label>
						</Label>
					</View>
				</View>
			</View>
			<View id="sudoWrapper">
				<TableView id="table" />
			</View>
			<View id="bottomWrapper">
				<View id="confirmView" onClick="checkSudoku">
					<Label id="btnConfirm">validate</Label>
				</View>
			</View>
		</View>
	</Window>
</Alloy>